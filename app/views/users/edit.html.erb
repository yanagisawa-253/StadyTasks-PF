<div class="container user-edit">
  <div class-"row">
    <div class="user-edit-box col-sm-6 text-center mx-auto mt-5 mb-5">
       <%= form_with model:@user, local:true do |f| %>
      <div class="user-image form-group">
        <%= attachment_image_tag @user, :user_image, :fill, 200, 200, fallback: "shutterstock_1013274682.png", class: "rounded-circle" %><br>
              <!--エラー箇所-->
        <%= f.attachment_field :user_image, placeholder: "プロフィール画像", class: "mt-3" %>
      </div>
      <table class="table">
        <tbody>
          <tr>
            <td> 名前</td>
            <td><%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前", :size => "30" %></td>
          </tr>
          <tr>
            <td>  自己紹介</td>
            <td><%= f.text_area :introduction, id:"inputIntroduction", placeholder:"自己紹介", :size => "30" %></td>
          </tr>
        </tbody>
      </table>
      <%= f.submit "変更を保存", class: "btn btn-secondary mx-auto" %>

      <% end %>
      <p class="text-secondary mt-5 mb-5">Profire.edit</p>
    </div>
  </div>
</div>